
  <%= render 'shared/errors', obj: @student %>
 <div class="container-fluid">
  <div class="  row ">
  <div class="col-sm-10 col-md-offset-2">
    <%= form_for(@student,:html =>{class:"form-horizontal",multipart: true ,role:"form"}) do |f| %>
  
       <div class="form-group" >
          <div class="control-label col-sm-2">
           <%= f.label :studentName,"Student Name" %>
          </div>
           <div class="col-sm-5">
             <%= f.text_field :studentName,placeholder:"Enter Student Name",class:"form-control",autofocus:true %>
          </div>
      </div>
       <div class="form-group" >
          <div class="control-label col-sm-2">
           <%= f.label :registration,"RegistrationNo" %>
          </div>
           <div class="col-sm-5">
             <%= f.number_field :registration,:in=>10114000..10114060,:step=>1,placeholder:"Enter RegistrationNo ",class:"form-control",autofocus:true %>
          </div>
      </div>
       <div class="form-group" >
          <div class="control-label col-sm-2">
           <%= f.label :Dob,"Date of Birth "%>
          </div>
           <div class="col-sm-5">
             <%= f.date_select :Dob, :start_year =>Time.now.year-30, :end_year => Time.now.year+10,class:"form-control",autofocus:true %>
          </div>
      </div>
       
       <div class="form-group" >
          <div class="control-label col-sm-2">
           <%= f.label :branch,"Stream"  %>
          </div>
           <div class="col-sm-5">
             <%= f.text_field :branch ,placeholder:"Enter Branch ",class:"form-control",autofocus:true %>
          </div>
      </div>
      <div class="form-group" >
          <div class="control-label col-sm-2">
           <%= f.label :cgpa,"CGPA" %>
          </div>
           <div class="col-sm-5">
             <%= f.number_field :cgpa,:in=> 1.0..10.0,:step =>0.01 ,placeholder:"Enter CGPA ",class:"form-control",autofocus:true %>
          </div>
      </div>

      <div class="form-group" >
          <div class="control-label col-sm-2">
           <%= f.label :subject,"Subjects" %>
          </div>
           <div class="col-sm-5">
             <%= f.text_area :subject,rows:6,placeholder:"Enter Subjects ",class:"form-control",autofocus:true %>
          </div>
      </div>
  
        <div class="form-group" >
          <div class="control-label col-sm-2">
           <%= f.label :language,"Language" %>
          </div>
           <div class="col-sm-5">
             <%= f.text_area :language,rows:6, placeholder:"Enter Languages ",class:"form-control",autofocus:true %>
          </div>
      </div>
  
       <div class="form-group" >
          <div class="control-label col-sm-2">
           <%= f.label :contact,"Contact Number" %>
          </div>
           <div class="col-sm-5">
             <%=  f.telephone_field :contact,placeholder:"Enter ContactNo ",class:"form-control",autofocus:true %>
          </div>
      </div>
  
       <div class="form-group" >
          <div class="control-label col-sm-2">
           <%= f.label :email ,"Email" %>
          </div>
           <div class="col-sm-5">
             <%= f.email_field :email,placeholder:"Enter Email ",class:"form-control",autofocus:true %>
          </div>
      </div>
 
       <div class="form-group" >
          <div class="control-label col-sm-2">
           <%= f.label :address,"Address" %>
          </div>
           <div class="col-sm-5">
             <%= f.text_area :address ,rows:6,placeholder:"Enter Address ",class:"form-control",autofocus:true %>
          </div>
      </div>

   <div class="form-group">
          <div class="control-label col-sm-2">
            <%= f.label :password %>
          </div>
           <div class="col-sm-5">
            <%= f.password_field :password, class: "form-control", placeholder: "Enter password" %>
          </div>
      </div>

     <div class="form-group">
       <div class="col-sm-5 col-sm-offset-2">
        <%= f.submit class:'btn btn-primary btn-lg'%>
      </div>
     </div>
<% end %>
</div>
</div>
</div>
