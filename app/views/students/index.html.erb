<div class="container-fluid">
<h1 align="center">Students</h1>
<table class="table table-hover table-bordered info">
  <thead>
    <tr>
      <th>Studentname</th>
      <th>RegistrationNo</th>
      <th>Dob</th>
      <th>Branch</th>
      <th>Cgpa</th>
      <th>Subject</th>
      <th>Language</th>
      <th>Contact</th>
      <th>Email</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td><%= student.studentName %></td>
         <td><%= student.registration %></td>
        <td><%= student.Dob %></td>
        <td><%= student.branch %></td>
        <td><%= student.cgpa %></td>
        <td><%= student.subject %></td>
        <td><%= student.language %></td>
        <td><%= student.contact %></td>
        <td><%= student.email %></td>
        <td><%= student.address %></td>
        <td><%= link_to 'Show', student , class: "btn btn-xs btn-primary"%></td>
        <td><%= link_to 'Edit', edit_student_path(student), class: "btn btn-xs btn-primary" %></td>
        <td><%= link_to 'Destroy', student, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-xs btn-primary" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<hr>
<div class="well">
  <div class="row">
     <div class="col-sm-12">
         <div class="col-sm-3">
             <button type="button" class="btn btn-success btn-md">
              <%= link_to 'New Student', new_student_path , class: "btn btn-xs btn-primary"%>
             </button>
         </div>
         <div class="col-sm-3">
             <button type="button" class="btn btn-success btn-md">
              <%=link_to 'Home',root_path, class: "btn btn-xs btn-primary"%>
             </button>
         </div>
     </div>

  </div>
</div>
</div>

