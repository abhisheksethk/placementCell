      <th>Company Name:</th>
      <th>HR Name:</th>
      <th>ContactNumber:</th>
      <th>Address:</th>
      <th>Email:</th>
       <th>Website:</th>
    </tr>
  </thead>
  <tbody>
      <tr>
        <td><%=@company.compName %></td>
        <td><%= @company.hrName %></td>
        <td><%=@company.contactNumber %></td>
        <td><%= @company.compPostalAddress %></td>
        <td><%= @company.email%></td>
        <td><%=  @company.website%></td>
      </tr>
  </tbody>
</table>
</div>
</div>
<hr>
<%=render '/jobs/job', obj: @company.jobs %>



